<?php
$x = rand(0,100);
echo 'число '.$x . '<br>';
$a = 1;
$b = 1;
while(true){
    if($a > $x){
        echo 'задуманное число Не входит числовой ряд';
        break;
    } elseif($a == $x){
        echo 'задуманное число входит в числовой ряд';
        break;
    }else{
    $c = $a;
    $a += $b;
    $b = $c;
    }
}
?>
